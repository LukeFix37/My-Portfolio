{
  "css.validate": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.includeLanguages": {
    "plaintext": "html",
    "typescript": "html"
  },
  "tailwindCSS.experimental.classRegex": [
    ["class\\s*=\\s*\"([^\"]*)", 1],
    ["\\[ngClass\\]=\\s*\"([^\"]*)", 1],
    ["\\[class\\]\\=\\s*\"([^\"]*)", 1]
  ]
}
